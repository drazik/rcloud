{# src/RCloud/Bundle/UserBundle/Resources/views/Dashboard/show.html.twig #}

{% extends '::base.html.twig' %}

{% block body %}
    <nav class="dashboard">
        {% for item in dashboardItems %}
            <a class="dashboard-item" href="{{ item.href }}">
                <i class="dashboard-item-icon fa fa-{{ item.icon }}"></i>
                <span class="dashboard-item-name">{{ item.title }}</span>
            </a>
        {% endfor %}
    </nav>
{% endblock %}
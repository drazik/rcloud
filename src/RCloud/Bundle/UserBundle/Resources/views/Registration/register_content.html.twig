{% trans_default_domain 'FOSUserBundle' %}

<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
    {{ form_widget(form) }}
    <div>
        <button class="button" type="submit">{{ 'registration.submit'|trans }}</button>
        <a class="button button-secondary" href="{{ path('fos_user_security_login') }}">Connexion</a>
    </div>
</form>
